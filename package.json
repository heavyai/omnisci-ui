{
  "name": "@mapd/omnisci-ui",
  "version": "0.1.1",
  "description": "OmniSci component ui library",
  "author": "OmniSci",
  "main": "dist/omnisci-ui.js",
  "types": "dist/index.d.ts",
  "contributors": [
    {
      "name": "Christophe Viau",
      "email": "christopheviau@gmail.com"
    }
  ],
  "scripts": {
    "build": "npm run build:prod && npm run build:dev",
    "build:prod": "npm run webpack:prod && npm run ts:prod",
    "build:dev": "npm run webpack:dev",
    "prod": "webpack --env.prod --watch",
    "dev": "webpack --env.dev --watch",
    "clean": "npm cache clean && rm -rf node_modules",
    "test": "jest --config jest.config.json",
    "test:snapshot": "jest --config jest.config.json --updateSnapshot",
    "lint": "eslint src --cache --ext=js --ext=jsx --ext=ts --ext=tsx",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write '{src,test}/**/*.{js,jsx,ts,tsx}' && npm run lint:fix",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "ts:prod": "tsc --emitDeclarationOnly",
    "ts:check": "tsc --noEmit",
    "webpack:dev": "SASS_PATH=./node_modules webpack --env.dev",
    "webpack:prod": "SASS_PATH=./node_modules webpack --env.prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/omnisci/omnisci-ui"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/omnisci/omnisci-ui/issues"
  },
  "homepage": "https://github.com/omnisci/omnisci-ui",
  "browserslist": [
    "defaults",
    "Explorer 11"
  ],
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.0",
    "@material/button": "^2.1.1",
    "@material/react-floating-label": "^0.13.0",
    "@material/typography": "^2.3.0",
    "@material/ripple": "^1.1.0",
    "@rmwc/button": "4.0.6",
    "@rmwc/dialog": "4.0.6",
    "@rmwc/icon": "4.0.6",
    "@rmwc/icon-button": "4.0.6",
    "@rmwc/line-ripple": "4.0.6",
    "@rmwc/list": "4.0.6",
    "@rmwc/menu": "4.0.6",
    "@rmwc/select": "4.0.6",
    "@rmwc/slider": "4.0.6",
    "@rmwc/switch": "4.0.6",
    "@rmwc/textfield": "4.0.6",
    "@rmwc/theme": "4.0.6",
    "@typescript-eslint/parser": "^1.9.0",
    "classnames": "^2.2.6",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-dates": "^20.1.0",
    "react-dom": "^16.8.6",
    "react-select": "^2.4.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/classnames": "^2.2.7",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.10.4",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.4",
    "autoprefixer": "^9.4.9",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chai-enzyme": "^1.0.0-beta.1",
    "chai-spies": "^1.0.0",
    "css-loader": "2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^5.15.0",
    "extract-text-webpack-plugin": "4.0.0-alpha.0",
    "identity-obj-proxy": "^3.0.0",
    "ink-docstrap": "^1.3.2",
    "jest": "^24.1.0",
    "jsdoc": "^3.6.2",
    "jsdom": "^13.2.0",
    "json-loader": "^0.5.7",
    "mocha": "^6.1.4",
    "mocha-webpack": "^2.0.0-beta.0",
    "node-sass": "^4.11.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "react-scripts": "^2.1.5",
    "react-styleguidist": "^9.0.2",
    "requirejs-plugins": "^1.0.2",
    "sanitize-html": "^1.20.0",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "text": "requirejs/text",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-node-externals": "^1.7.2",
    "yargs": "^13.2.1"
  }
}
